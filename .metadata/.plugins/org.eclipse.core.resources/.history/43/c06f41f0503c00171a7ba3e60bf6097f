package managementsoftware;
import java.io.*;
import java.util.*;
import java.lang.*;
public class DuplicatesOfArray {
	public static boolean IsDuplicate(int[] arr)
	{
		boolean _isDuplicate = false;
		HashMap<Integer, Integer> hmap = new HashMap<Integer,Integer>();
		int count =0;
       for(Integer i: arr)
       {
    	   if(!hmap.containsKey(i))
    	   {
    		   count = 1;
    		   hmap.put(i,count);
    	   }
    	   else
    	   {
    		   hmap.put(i, count++);
    	   }
       }
		
		
		
		return _isDuplicate;
	}
public static void main(String[] args)
{
	int[] arr = new int[]{10,30,10,40};
	
	
	
	
}
}
